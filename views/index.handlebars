<ul>
  {{#each articles}}
  	<div>
    <li>
      <p> Article Title :  {{title}}</p>
      <p> Link : {{link}}</p>

      <form action="/article/{{this._id}}/note" method="post">
      <input type ="text" name="title"/>
      <input type ="submit" value = "Add note">
      </form>

   
    </li>
    </div>
  {{/each}}
</ul>



<ul>
  {{#each note}}
  	<div>
    <li>
      <p> Article Title :  {{title}}</p>
      <p> Link : {{body}}</p>
   
    </li>
    </div>
  {{/each}}
</ul>